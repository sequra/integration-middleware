{
    "name": "sequra/middleware",
    "description": "SeQura middleware library for cloud based integrations.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^10.10",
        "sequra/integration-core": "1.0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "SeQura\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeQura\\Middleware\\Tests\\": "tests/",
            "SeQura\\Core\\Tests\\": "vendor/sequra/integration-core/tests/"
        }
    },
    "license": "GPL",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "git@github.com:sequra/integration-core.git"
        }
    ]
}
